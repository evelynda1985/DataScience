    "nums = [1,35,12,24,31,51,70,100]\n",
    "def count(nums):\n",
    "    increment = len(nums) -1 \n",
    "    count = 0\n",
    "    while increment >= 0:\n",
    "        if nums[increment] < 20:\n",
    "            count += 1\n",
    "        increment -=1\n",
    "    return count"
   ]
